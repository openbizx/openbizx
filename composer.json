{
    "name": "openbizx/openbizx",
    "description": "Openbizx PHP Framework",
    "keywords": [
        "openbizx",
        "framework",
		"metadata",
		"enterprise"
    ],
    "homepage": "http://openbizx.github.io/",
    "type": "library",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Agus Suhartono",
            "email": "agus.suhartono@gmail.com",
            "homepage": "http://www.binexa.com/",
            "role": "co-Founder and project lead"
        },
        {
            "name": "Openbizx Community",
            "homepage": "https://github.com/openbizx/openbizx/graphs/contributors"
        }		
    ],
    "support": {
        "issues": "https://github.com/openbizx/openbizx/issues?state=open",
        "forum": "https://groups.google.com/d/forum/openbizx",
        "wiki": "https://github.com/openbizx/openbizx/wiki",
        "source": "https://github.com/openbizx/openbizx"
    },
    "minimum-stability": "dev",
    "replace": {
        "openbizx/openbizx": "self.version"
    },
    "require": {
        "php": ">=5.4.0",
        "ext-mbstring": "*",
        "lib-pcre": "*",
        "zf1/zend-view": "*",
		"zf1/zend-db" : "*",
		"zf1/zend-cache" : "*",
		"zf1/zend-mail" : "*",
		"zf1/zend-log" : "*",
		"zf1/zend-validate" : "*",
		"zf1/zend-json" : "*",
		"smarty/smarty" : "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.4",
        "cebe/indent": "*"
    },
    "autoload": {
        "psr-4": {
            "Openbizx\\": "src/"
        }
    }
}
